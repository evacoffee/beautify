{
  "schemaVersion": 1,
  "id": "beautymod",
  "version": "${version}",
  "name": "Beauty Mod",
  "description": "A mod that adds relationship and dating mechanics to Minecraft",
  "authors": ["YourUsername"],
  "contact": {
    "homepage": "https://github.com/yourusername/beautymod",
    "sources": "https://github.com/yourusername/beautymod",
    "issues": "https://github.com/yourusername/beautymod/issues"
  },
  "license": "MIT",
  "icon": "assets/beautymod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.evacoffee.beautymod.BeautyMod"
    ],
    "client": [
      "com.evacoffee.beautymod.BeautyModClient"
    ],
    "modmenu": [
      "com.evacoffee.beautymod.config.ModMenuIntegration"
    ],
    "cardinal-components": [
      "com.evacoffee.beautymod.components.ModComponents"
    ]
  },
  "mixins": [
    "beautymod.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.0",
    "fabric": "*",
    "minecraft": "1.19.x",
    "java": ">=17",
    "cloth-config": ">=8.0.0",
    "fabric-permissions-api-v0": "*"
  },
  "suggests": {
    "modmenu": ">=4.0.0"

    "entrypoints": {
  "cardinal-components": [
    "com.evacoffee.beautymod.components.ModComponents"
  ]
  
  },
  "custom": {
    "cardinal-components": [
      "beautymod:player_data",
      "beautymod:dating",
      "beautymod:marriage"
    ],
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/yourdiscord",
        "modmenu.wiki": "https://github.com/yourusername/beautymod/wiki"
      }
    }
  },
  "breaks": {
    "optifabric": "*"
  }
}