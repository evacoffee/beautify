{
  "schemaVersion": 1,
  "id": "beautymod",
  "version": "${version}",
  "name": "BeautyMod",
  "description": "Enhances relationships and social interactions in Minecraft",
  "authors": [
    "YourName"
  ],
  "contact": {
    "homepage": "https://github.com/yourusername/beautymod",
    "sources": "https://github.com/yourusername/beautymod",
    "issues": "https://github.com/yourusername/beautymod/issues"
  },
  "license": "MIT",
  "icon": "assets/beautymod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.evacoffee.beautymod.BeautyMod"
    ],
    "client": [
      "com.evacoffee.beautymod.client.BeautyModClient"
    ],
    "server": [
      "com.evacoffee.beautymod.server.BeautyModServer"
    ],
    "modmenu": [
      "com.evacoffee.beautymod.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "beautymod.mixins.json"
  ],
  "accessWidener": "beautymod.accesswidener",
  "depends": {
    "fabricloader": ">=0.14.0",
    "fabric": "*",
    "minecraft": "1.20.x",
    "java": ">=17",
    "fabric-permissions-api-v0": "*",
    "cloth-config": ">=10.0.0"
  },
  "suggests": {
    "modmenu": ">=4.0.0",
    "cloth-config": ">=10.0.0"
  },
  "recommends": {
    "modmenu": ">=4.0.0",
    "cloth-config": ">=10.0.0"
  },
  "breaks": {
    "optifabric": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/yourdiscord",
        "modmenu.wiki": "https://github.com/yourusername/beautymod/wiki"
      }
    },
    "cardinal-components": [
      "beautymod:player_data"
    ]
  }
}